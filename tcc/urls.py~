# -*- coding: utf-8 -*-



from django.conf.urls import patterns, include, url


from django.contrib import admin
admin.autodiscover()

urlpatterns = patterns('',
    # Examples:
    # url(r'^$', 'tcc.views.home', name='home'),

    
    url(r'^admin/', include(admin.site.urls)),
    url(r'^456/$', 'historicos.views.f2'),
    url(r'^$', 'historicos.views.show_users', name='home'),
    url(r'^usuarios/$', 'historicos.views.show_users', name="user.all"),
    url(r'^usuarios/(?P<usuario>\D{3,15})/$', 'historicos.views.show_histories', name="user.history"),
    url(r'^usuarios/(?P<usuario>\D{3,15})/(?P<ano>\d{4})/$', 'historicos.views.show_histories_year', name='user.history.year'),
    url(r'^usuarios/(?P<usuario>\D{3,15})/(?P<ano>\d{4})/(?P<mes>\d{1,2})/$', 'historicos.views.show_histories_year_month', name='user.history.year.month'),
    url(r'^usuarios/(?P<usuario>\D{3,15})/(?P<ano>\d{4})/(?P<mes>\d{1,2})/(?P<dia>\d{1,2})/$', 'historicos.views.show_histories_year_month_day', name='user.history.year.month.day'),

)


